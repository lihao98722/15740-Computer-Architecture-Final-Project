[PIN TRACK SEETINGS]
Track_Individual_Instructions 	= 0  // -ti 
Track_Individual_Loads        	= 0  // -tl 
Track_Individual_Stores	  	= 0  // -ts 


[CACHE CONFIGURATION]
Cache_Size			= 32 // -c
Cache_Line_Size			= 32 // -b
Cache_Associativity		= 4  // -a
Cache_Coherence_Type		= 1  // -ct (0:Dir Based
				     //	     1:Snoop Write_Invalidate
				     //	     2:Snoop MSI 
				     //	     3:Snoop MESI)
 
[THRESHOLDS]
Instruction_Miss_Threshold 	= 1 	// -im default = 0 (not used)
Instruction_Hit_Threashold 	= 1 	// -ih default = 0 (not used)
Data_Miss_Threshold     	= 100 	// -rm (Currently Not Used)
Data_Hit_Threshold		= 100 	// -rh (Currently Not Used)

[PIN TOOL]
Name				= SMPCache

[BENCHMARK]
Name				= pthread1
